{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deploymentContext": { "type": "object" },
    "source": { "type": "object" },
    "context": { "type": "object" }
  },
  "variables": {
    "instance": {
      "settings": {
        "fileUris": "[parameters('source').settingsConfig.fileUris]",
        "commandToExecute": "[concat('powershell -ExecutionPolicy Unrestricted -Command \"& {.\\gmsa.ps1 -AdminUser \\\"', parameters('source').settingsConfig.activeDirectorySettings.adminUser, '\\\" -AdminPassword \\\"', parameters('source').settingsConfig.activeDirectorySettings.adminPassword, '\\\" -NetBiosDomainName \\\"', parameters('source').settingsConfig.activeDirectorySettings.netBiosDomainName, '\\\" -FqDomainName \\\"', parameters('source').settingsConfig.activeDirectorySettings.fqDomainName, '\\\" -GmsaName \\\"', parameters('source').settingsConfig.activeDirectorySettings.gmsaName, '\\\" -FederationName \\\"', parameters('source').settingsConfig.activeDirectorySettings.federationName, '\\\" -HostName \\\"', parameters('source').settingsConfig.activeDirectorySettings.hostName, '\\\" -HostIp \\\"', parameters('source').settingsConfig.activeDirectorySettings.hostIp, '\\\"}\"')]"
      },
      "protectedSettings": { }
    }
  },
  "resources": [

  ],
  "outputs": {
    "settings": {
      "type": "object",
      "value": "[variables('instance')]"
    }
  }
}
